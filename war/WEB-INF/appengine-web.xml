<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>lingr-bot</application>
	<version>2011-01-12a</version>

	<static-files>
		<include path="*.html" />
		<include path="*.ico" />
		<include path="gree/*.xml" />
		<include path="css/**.*" />
		<include path="js/**.*" />
		<include path="images/**.*" />
	</static-files>
	<resource-files>
		<include path="/testfile/**.*" />
	</resource-files>
	<sessions-enabled>true</sessions-enabled>


	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/classes/logging.properties" />
	</system-properties>
</appengine-web-app>
